server:
  port: 8088
spring:
  application:
    name: api-gateway
  cloud:
    gateway:
      #路由规则
      routes:
        - id: order_route # 路由的唯一标识，路由到order
          uri: http://localhost:8020 #需要转发的地址
          #断言规则 用于路由规则的匹配
          predicates:
            - Path=/order-serv/**
              #  http://localhost:8088/order-serv/order/add↓
              #  http://localhost:8020/order-serv/order/add
          filters:
            - StripPrefix=1 # 转发之前去掉第一层路径
#        - id: